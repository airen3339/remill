+claim_eq(remill_please_dont_use_this_temp_name267, $(INST_NEXT_PTR));
+
 	if (RESERVE == 0) goto inst_next;
+
 	*[ram]:8 EA = storeDoubleWordConditionalIndexed(S,RA_OR_ZERO,B);
